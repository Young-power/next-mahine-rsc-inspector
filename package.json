{
  "name": "next-mahine-rsc-inspector",
  "version": "1.0.1",
  "description": "Inspect and analyze Next.js React Server Components architecture",
  "keywords": [
    "nextjs",
    "react",
    "rsc",
    "cli",
    "analysis",
    "architecture",
    "inspector"
  ],
  "author": "Mahaman Mahine Kamagat√©",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "next-mahine-rsc-inspector": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts src/cli.ts --dts --watch"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "log-symbols": "^7.0.1",
    "nanospinner": "^1.2.2",
    "strip-ansi": "^7.1.2"
  }
}
